@model List<Model.EF.SNO_Job>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Theme3.cshtml";
}

<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>Item Code</th>
            <th width="150px">Start</th>
            <th width="150px">End</th>
            <th width="170px">Due</th>
            <th width="80px">Check</th>
            <th width="80px">Machine</th>
            <th width="80px">Late</th>
            <th width="80px">Amount</th>
        </tr>
    </thead>
    <tbody id="gvCustomers2">
        @if (Model != null)
        {


            foreach (var item in Model)
            {

                <tr id="row_@item.Item">
                    <td>@item.Item</td>
                    <td>@item.StartDate</td>
                    <td>@item.EndDate</td>
                    <td>@item.DueDate</td>
                    <td>@item.getjob</td>
                    <td>@item.MachineID</td>
                    <td>@item.Late</td>
                    <td>@item.Amount</td>
                </tr>
            }
        }

    </tbody>
</table>